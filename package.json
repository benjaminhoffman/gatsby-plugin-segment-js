{
  "name": "gatsby-plugin-segment-js",
  "description": "Easily add Segment JS snippet to your website",
  "author": "benjamin hoffman <6520022+benjaminhoffman@users.noreply.github.com>",
  "license": "MIT",
  "version": "3.7.1",
  "main": "gatsby-ssr.js",
  "homepage": "https://github.com/benjaminhoffman/gatsby-plugin-segment-js#readme",
  "repository": "git@github.com:benjaminhoffman/gatsby-plugin-segment-js.git",
  "bugs": {
    "url": "https://github.com/benjaminhoffman/gatsby-plugin-segment-js/issues"
  },
  "keywords": [
    "segment",
    "segment analytics",
    "segment gatsby",
    "segment javascript",
    "analytics.js",
    "gatsby",
    "gatsby segment",
    "gatsby-plugin",
    "gatsby-plugin segment"
  ],
  "scripts": {
    "build:browser": "babel --out-dir . --config-file ./babel-browser.config.js src/*-browser.js",
    "build:ssr": "babel --out-dir . --config-file ./babel-ssr.config.js src/*-ssr.js",
    "build": "yarn build:browser && yarn build:ssr",
    "prepare": "yarn build",
    "build-n-pack": "yarn build && yarn pack --filename ./gatsby-plugin-segment-js.tgz",
    "watch": "babel -w src --out-dir . --ignore tests"
  },
  "peerDependencies": {
    "gatsby": "^2 || ^3 || ^4"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "eslint": "^7.32.0",
    "eslint-plugin-mocha": "^9",
    "eslint-plugin-react": "^7.24.0"
  }
}
